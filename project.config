[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Matt W (mattw-db)
[access "refs/for/refs/*"]
	read = group user/Matt W (mattw-db)
	push = group user/Matt W (mattw-db)
	label-Code-Review = -2..+2 group user/Matt W (mattw-db)
	label-Verified = -1..+1 group user/Matt W (mattw-db)
	rebase = group user/Matt W (mattw-db)
	removeReviewer = group user/Matt W (mattw-db)
	submit = group user/Matt W (mattw-db)
[access "refs/heads/*"]
	read = group user/Matt W (mattw-db)
	create = group user/Matt W (mattw-db)
	push = +force group user/Matt W (mattw-db)
	pushMerge = group user/Matt W (mattw-db)
[access "refs/tags/*"]
	push = +force group user/Matt W (mattw-db)
	pushTag = group user/Matt W (mattw-db)
	pushSignedTag = group user/Matt W (mattw-db)
